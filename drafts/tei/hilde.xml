			<pb type="manuscript" n="1r" />
			<lb type="manuscript" n="1" />
			<div n="1" xml:id="verse-1" type="verse" data-level="1">Ik gihorta ðat seggen</div>
			<div n="2" xml:id="verse-2" type="verse" data-level="1">ðat sih urhettun ænon muo<lb type="manuscript" n="2" />tin ·</div>
